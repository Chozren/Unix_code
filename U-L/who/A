
void show_info(struct utmpx *utbufp)
{

    void show_time();

   // if (utbufp->ut_type != USER_PROCESS)
     //   return;
    printf("%-8.8s", utbufp->ut_user);     /* the logname*/
    printf(" ");
    printf("%-8.8s", utbufp->ut_line);     /* the tty */ 
    printf(" ");
    show_time();
# ifdef SHOWHOST
    if (utbufp->ut_host[0] != '\0')
        printf("(%s)", utbufp->ut_host);        /* the host */ 
# endif
    printf("\n");
} 
